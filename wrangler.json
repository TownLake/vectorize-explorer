{
  "version": 2,
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "build": {
    "command": "npm run build",
    "cwd": "."
  },
  "pages_build_output_dir": "dist",
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "blog-posts"
    }
  ],
  "ai": {
    "binding": "AI"
  }
}
