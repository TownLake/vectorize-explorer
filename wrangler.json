{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-next-app",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "pages_build_output_dir": ".vercel/output/static",
  "observability": {
    "enabled": true
  },
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "blog-posts"
    }
  ],
  "ai": {
    "binding": "AI"
  }
}
